{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "CalmZone",
    "slug": "CalmZone",
    "scheme": "com.calmzone.authsessiongoogle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/flash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4195AF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calmzone.authsessiongoogle",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "com.calmzone.authsessiongoogle",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/flash.png",
        "backgroundColor": "#4195AF"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "101300a7-ad4f-4c25-bfb5-a3b165a39df4"
      }
    }
  }
}
